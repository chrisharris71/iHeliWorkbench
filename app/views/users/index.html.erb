<%= provide(:title, 'All users') %>
<h1>All users</h1>
<% if @users.previous_page %>
  <%= link_to "< Previous", params.merge(:page => @users.previous_page) %>
<% end %>
Page <%= @users.current_page %> of <%= @users.total_pages %>
<% if @users.next_page %>
  <%= link_to "Next >", params.merge(:page => @users.next_page) %>
<% end %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>